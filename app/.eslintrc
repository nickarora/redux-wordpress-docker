{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "strict": 0,
    "no-undef": 2,
    "max-len": [1, 100, 2, {
      "ignoreUrls": true
    }],
    "semi": [2, "never"],
    "no-confusing-arrow": 0,
    "no-unexpected-multiline": 2,
    "jsx-quotes": [2, "prefer-single"],
    "object-curly-spacing": [2, "always"],
    "array-bracket-spacing": [2, "never"],
    "space-before-function-paren": [2, "never"],
    "object-shorthand": 1,
    "prefer-template": 1,
    "no-param-reassign": 1,
    "consistent-return": 0,
    "array-callback-return": 1,
    "quote-props": 1,
    "arrow-body-style": 1,
    "prefer-const": 1,
    "no-return-assign": 1
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.babel.js"
      }
    }
  }
}